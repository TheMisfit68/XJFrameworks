   <SourceLine>Function lookupRecord(lookupTable as String,lookupField as String, lookupValue as string) As Recordset</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim previousRecords as Recordset = SQLSelect("Select ID From "+lookupTable+"' WHERE "+lookupField+"='"+lookupValue+"';")</SourceLine>
   <SourceLine>previousRecords.MoveFirst</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return previousRecords</SourceLine>
   <SourceLine>End Function</SourceLine>
